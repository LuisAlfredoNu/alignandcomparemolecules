#!/usr/local/bin/vmd
# VMD script written by save_state $Revision: 1.47 $
# VMD version: 1.9.3
set viewplist {}
set fixedlist {}
# Display settings
display projection   Orthographic
display height       4.000000
display distance     -2.000000
display nearclip set 0.000000
display depthcue   off
display shadows off
display ambientocclusion off
display aoambient 0.800000
display aodirect 0.300000
display dof off
display dof_fnumber 64.000000
display dof_focaldist 0.700000
draw text {5 0 0} "Different"
mol new R-S-S-Fructuose_out_G09.xyz type xyz first 0 last -1 step 1 filebonds 1 autobonds 1 waitfor all
mol delrep 0 top
mol representation CPK 1.000000 0.400000 150.000000 150.000000
mol color Name
mol selection {all}
mol material Opaque
mol addrep top
mol scaleminmax top 0 0.000000 0.000000
mol smoothrep top 0 0
mol drawframes top 0 {now}
draw color blue
draw cylinder {0 0 0} {2.99965 0.0428386 -0.0171536} radius 0.05
draw color red
draw cylinder {0 0 0} {0.0429207 -2.99966 0.014325} radius 0.05
draw color green
draw cylinder {0 0 0} {0.0169471 0.0145687 2.99992} radius 0.05
mol rename molecule_A_R-S-S-Fructuose_out_G09.xyz
# done with molecule 0
mol new S-R-S-Fructuose_out_G09.xyz type xyz first 0 last -1 step 1 filebonds 1 autobonds 1 waitfor all
mol delrep 0 top
mol representation CPK 1.000000 0.400000 150.000000 150.000000
mol color Name
mol selection {all}
mol material Opaque
mol addrep top
mol scaleminmax top 0 0.000000 0.000000
mol smoothrep top 0 0
mol drawframes top 0 {now}
draw color blue3
draw cylinder {0 0 0} {-2.99968 0.0435643 -0.000979152} radius 0.05
draw color red3
draw cylinder {0 0 0} {-0.0435545 -2.99958 -0.0253366} radius 0.05
draw color green3
draw cylinder {0 0 0} {-0.00134694 -0.0253197 2.99989} radius 0.05
mol rename molecule_B_S-R-S-Fructuose_out_G09.xyz
# done with molecule 0
proc vmdrestoremycolors {} {
	set colorcmds {
		{color Display {Background} white}
		{color Display {BackgroundTop} black}
		{color Display {BackgroundBot} blue2}
		{color Display {FPS} black}  
	}
	foreach colcmd $colorcmds {    
		set val [catch {eval $colcmd}]  
	}
}
vmdrestoremycolors
